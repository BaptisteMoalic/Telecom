<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pact31">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PACT31">
        <activity android:name=".controller.PoiActivity"></activity>
        <activity android:name=".controller.BraceletsActivity" />
        <activity android:name=".controller.ParamrouteActivity" />
        <activity android:name=".controller.EscalesActivity" />
        <activity android:name=".controller.TransportActivity" />
        <activity android:name=".controller.ItineraireActivity" />
        <activity android:name=".controller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>